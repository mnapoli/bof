{
    "name": "mnapoli/bof",
    "description": "The HTTP client for humans",
    "keywords": ["http", "client", "http client"],
    "license": "MIT",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Bof\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bof\\Test\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.3",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.4",
        "guzzlehttp/psr7": "^1.6",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "maglnet/composer-require-checker": "^2.0",
        "mnapoli/hard-mode": "^0.2.0",
        "vimeo/psalm": "^3.6"
    }
}
